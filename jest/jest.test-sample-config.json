{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": "../src/test-sample",
  "testRegex": ".*\\.spec\\.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "collectCoverageFrom": [
    "**/*.{!(module),}.(t|j)s"
  ],
  "coverageDirectory": "/coverage",
  "testEnvironment": "node"
}
