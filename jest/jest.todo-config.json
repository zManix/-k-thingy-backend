{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": "../src/todo",
  "testRegex": ".*\\.spec\\.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "collectCoverageFrom": [
    "**/*.{!(module),}.(t|j)s"
  ],
  "coverageDirectory": "../../coverage",
  "coveragePathIgnorePatterns": [
    ".*intercept.*",
    ".*middleware.*",
    ".*decorator.*",
    ".*filter.*",
    ".*jwt-api-auth-guard.service.*",
    ".*module.*",
    ".*entities.*",
    ".*dto.*",
    ".*index.*",
    ".*main.*"
  ],
  "testEnvironment": "node"
}
